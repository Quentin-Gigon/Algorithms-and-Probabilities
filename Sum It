class Main {
    public static void main(String[] args) {
        // Uncomment this line if you want to read from a file
        In.open("public/sample.in");
        
        int t = In.readInt();
        for (int i = 0; i < t; i++) {
            testCase();
        }
        
        // Uncomment this line if you want to read from a file
         In.close();
    }

    public static void testCase() {
        // Input using In.java class
        //Initialize array with numbers to sum
        int n = In.readInt();
        int[] tosum = new int[n];
        int i = 0;
        while (i<n){
          tosum[i] = scanner.nextInt();
          i++;
        }
        
        //Sum all numbers in array and return sum
        int sum;
        for(int i=0;i<n;i{
          sum += tosum[i];
        }
        System.out.println(sum);
        
        
        // Output using Out.java class
        Out.println(n + n);
    }
}
